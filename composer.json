{
  "name": "decotatoo/woocommerce-integration",
  "description": "WooCommerce integration for ERP",
  "type": "library",
  "require": {
    "php": "^8.0",
    "codexshaper/laravel-woocommerce": "dev-master",
    "dvdoug/boxpacker": "^3.9",
    "illuminate/contracts": "^8.37",
    "spatie/laravel-package-tools": "^1.9.2"
  },
  "require-dev": {
    "nunomaduro/collision": "^5.10",
    "nunomaduro/larastan": "^1.0",
    "orchestra/testbench": "^6.0",
    "pestphp/pest": "^1.10",
    "pestphp/pest-plugin-laravel": "^1.1",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0",
    "phpunit/phpunit": "^9.4",
    "spatie/laravel-ray": "^1.26"
  },
  "license": "MIT",
  "autoload": {
      "psr-4": {
          "Decotatoo\\WoocommerceIntegration\\": "src/",
          "Decotatoo\\WoocommerceIntegration\\Database\\Factories\\": "database/factories",
          "Decotatoo\\WoocommerceIntegration\\Database\\Seeders\\": "database/seeders/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Decotatoo\\WoocommerceIntegration\\Tests\\": "tests"
      }
  },
  "scripts": {
      "phpstan": "vendor/bin/phpstan analyse",
      "test": "vendor/bin/pest",
      "test-coverage": "vendor/bin/pest coverage"
  },
  "authors": [
      {
          "name": "decotatoo",
          "email": "it@deco-tatoo.com"
      }
  ],
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Decotatoo\\WoocommerceIntegration\\WoocommerceIntegrationServiceProvider"
          ],
          "aliases": {
              "WoocommerceIntegration": "Decotatoo\\WoocommerceIntegration\\WoocommerceIntegrationFacade"
          }
      }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
